@model proj1.Models.Customer
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
 <h4 class="text-success">
     Customer Details
 </h4>
 <div style="height:100px;background-color:aliceblue" class="container border">
     <div class="col-14">
         <div class="row" style="padding-top:10px;">
             <div class="form-group col-md-4">
                 <div class="col-7" style="display:flex;justify-content:center;align-content:center;">
                     <label>Full Name</label>
                 </div>
                 <div >
                     <input class="col-4" asp-for="@Model.CusName" readonly class="form-control" />
                 </div>
             </div>
             <div class="form-group col-md-4">
                 <div class="col-7" style="display:flex;justify-content:center;align-content:center;">
                     <label>Phone Number</label>
                     
                 </div>
                 <div >
                     <input class="col-6" asp-for="@Model.PhoneNum" readonly class="form-control" />
                 </div>
             </div>
             <div class="form-group col-md-4">
                 <div class="col-7" style="display:flex;justify-content:center;align-content:center;">
                     <label>Address</label>
                 </div>
                 <div >
                     <input class="col-6" asp-for="@Model.Address" readonly class="form-control" />
                 </div>
             </div>
         </div>
     </div>
 </div>