@model proj1.Models.ProductTypes
@{
    ViewData["Title"] = "Edit";
}
<br />
<h2 class="text-info"> Product Type Details</h2>

<div class="p-4 border rounded">
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="NameType"></label>
        </div>
        <div class="col-5">
            <input asp-for="NameType" disabled class="form-control" />
        </div>
        <span asp-validation-for="NameType" class="text-danger"></span>
    </div>
    <br />
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.TypeId" class="btn btn-primary">Edit</a>
        <a asp-action="Index" class="btn btn-success">Back to list</a>
    </div>
</div>

